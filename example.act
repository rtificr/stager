title: A NEW example ACT;
author: Jay Churchill;
description: This is an example ACT that demonstrates the basic structure of an ACT file.;
date: 2020-01-01;

let name;

start: What's your name? ~ confirmname (name = input());

confirmname: So your name is "<name>"? {
	(name != "") Yes ~ end;
	No ~ start;
};

end: Goodbye, <name>!;